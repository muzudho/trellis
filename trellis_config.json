{
    "builder": {

    },
    "compiler": {
        "objectFolder": "./temp/objects",
        "objectFilePrefix": null,
        "translationOrder": [
            "imports",
            "resolveVarBounds",
            "autoSplitSegmentByPillar",
            "autoShadow"
        ],
        "tlanslators": {
            "autoSplitSegmentByPillar": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_auto_split_pillar"
                }
            },
            "autoShadow": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_auto_shadow"
                }
            },
            "resolveVarBounds": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_resolve_var_bounds"
                }
            },
            "imports": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_imports"
                }
            }
        }
    },
    "renderer": {
        
    }
}