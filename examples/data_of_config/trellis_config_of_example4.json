{
    "builder": {
        "--source": "./examples/data_of_contents/battle_sequence_of_unfair_cointoss.step4_auto_shadow.json"
    },
    "compiler": {
        "objectFolder": "./temp/examples",
        "objectFilePrefix": null,
        "translationOrder": [
            "imports",
            "resolveAliasOfColor",
            "resolveVarBounds",
            "autoShadow"
        ],
        "tlanslators": {
            "autoShadow": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_auto_shadow"
                }
            },
            "resolveAliasOfColor": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_resolve_alias_of_color"
                }
            },
            "resolveVarBounds": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_resolve_var_bounds"
                }
            },
            "imports": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_imports"
                }
            }
        }
    },
    "renderer": {
        "--output": "./temp/examples/no4_auto_shadow.xlsx",
        "features": {
        }
    }
}