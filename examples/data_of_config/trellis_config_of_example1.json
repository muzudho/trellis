{
    "builder": {
        "--source": "./examples/data_of_contents/battle_sequence_of_unfair_cointoss.step1_full_manual.json"
    },
    "compiler": {
        "objectFolder": "./temp/examples",
        "objectFilePrefix": null,
        "translationOrder": [
            "imports",
            "resolveVarBounds"
        ],
        "tlanslators": {
            "resolveVarBounds": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_resolve_var_bounds"
                }
            },
            "imports": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_imports"
                }
            }
        }
    },
    "renderer": {
        "--output": "./temp/examples/no1_new_paper.xlsx",
        "features": {
            "canvas": {
                "enabled": true
            },
            "cards": {
                "enabled": false
            },
            "lineTapes": {
                "enabled": false
            },
            "pillars": {
                "enabled": false
            },
            "rectangles": {
                "enabled": false
            },
            "ruler": {
                "enabled": true
            },
            "shadowOfCards": {
                "enabled": false
            },
            "shadowOfLineTapes": {
                "enabled": false
            },
            "shadowOfTerminals": {
                "enabled": false
            },
            "terminals": {
                "enabled": false
            },
            "xlTexts": {
                "enabled": false
            }        
        }
    }
}