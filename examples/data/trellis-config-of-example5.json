{
    "builder": {
        "--source": "./examples/data/battle_sequence_of_unfair_cointoss.step5_auto_split_by_pillar.json"
    },
    "compiler": {
        "objectFolder": "./temp/examples",
        "objectFilePrefix": null,
        "tlanslators": {
            "autoSplitSegmentByPillar": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_auto_split_by_pillar"
                }
            },
            "autoShadow": {
                "enabled": true,
                "objectFile": {
                    "write": true,
                    "suffix": "after_auto_shadow"
                }
            }
        }
    },
    "renderer": {
        "--output": "./temp/examples/no5_auto_split_pillar.xlsx"
    }
}